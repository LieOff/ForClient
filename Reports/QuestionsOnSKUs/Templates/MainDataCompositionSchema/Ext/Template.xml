		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	VisitSKUs.Ref AS VisitRef,
	VisitSKUs.Question,
	VisitSKUs.Answer,
	QuestionsInQuestionnairesSliceLast.Questionnaire,
	VisitSKUs.SKU
INTO Questions
FROM
	Document.Visit.SKUs AS VisitSKUs
		LEFT JOIN InformationRegister.QuestionsInQuestionnaires.SliceLast(, ) AS QuestionsInQuestionnairesSliceLast
		ON VisitSKUs.Questionnaire = QuestionsInQuestionnairesSliceLast.Questionnaire
			AND VisitSKUs.Question = QuestionsInQuestionnairesSliceLast.ChildQuestion
WHERE
	NOT QuestionsInQuestionnairesSliceLast.Status = VALUE(Enum.ValueTableRowStatuses.Deleted)
	AND CASE
			WHEN &amp;КонецПериода = DATETIME(1, 1, 1)
				THEN TRUE
			ELSE QuestionsInQuestionnairesSliceLast.Period &lt;= &amp;КонецПериода
		END
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	Visit.Date,
	Visit.Outlet,
	Visit.SR,
	Visit.Ref
INTO Visits
FROM
	Document.Visit AS Visit
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	OutletList.Ref,
	OutletList.Type,
	OutletList.Class,
	OutletList.Distributor,
	OutletList.Address,
	OutletList.ReceivableLimit,
	TerritoryOutlets.Ref AS Territory,
	Territory.Owner AS Region
INTO Outlets
FROM
	Catalog.Territory.Outlets AS TerritoryOutlets
		LEFT JOIN Catalog.Outlet AS OutletList
		ON TerritoryOutlets.Outlet = OutletList.Ref
		LEFT JOIN Catalog.Territory AS Territory
		ON TerritoryOutlets.Ref = Territory.Ref
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	Visits.Date,
	Visits.Outlet,
	Visits.SR,
	Questions.Question,
	Questions.Answer,
	Questions.Questionnaire,
	Visits.Ref AS VS,
	Questions.SKU
INTO FirstJoin
FROM
	Questions AS Questions
		LEFT JOIN Visits AS Visits
		ON Questions.VisitRef = Visits.Ref
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	FirstJoin.Date,
	FirstJoin.Outlet,
	FirstJoin.SR,
	FirstJoin.Question,
	FirstJoin.Answer,
	Outlets.Ref,
	Outlets.Type,
	Outlets.Class,
	Outlets.Distributor,
	Outlets.Address,
	Outlets.ReceivableLimit,
	FirstJoin.Questionnaire,
	Outlets.Territory,
	Outlets.Region,
	FirstJoin.VS,
	FirstJoin.SKU
FROM
	FirstJoin AS FirstJoin
		LEFT JOIN Outlets AS Outlets
		ON FirstJoin.Outlet = Outlets.Ref
{WHERE
	(FirstJoin.Date &lt;= &amp;КонецПериода),
	(FirstJoin.Date &gt;= &amp;НачалоПериода)}</query>
