		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED DISTINCT
	Visit.Outlet,
	BEGINOFPERIOD(Visit.Date, DAY) AS Date,
	VisitSKUs.SKU,
	VisitSKUs.Answer AS Answer,
	Brands.Ref AS Brand,
	SKUGroup.Ref AS SKUGroup,
	Visit.Outlet.Class AS Class,
	Visit.Outlet.Type AS Type,
	Visit.Outlet.Distributor AS Distributor,
	Visit.SR
INTO Visits
FROM
	Document.Visit.SKUs AS VisitSKUs
		LEFT JOIN Document.Visit AS Visit
		ON VisitSKUs.Ref = Visit.Ref
		LEFT JOIN Catalog.Brands AS Brands
		ON VisitSKUs.SKU.Brand = Brands.Ref
		LEFT JOIN Catalog.SKUGroup AS SKUGroup
		ON VisitSKUs.SKU.Owner = SKUGroup.Ref
WHERE
	VisitSKUs.Question.Assignment = VALUE(Enum.SKUQuestions.Stock)
{WHERE
	(Visit.Date &gt;= &amp;StartDate),
	(Visit.Date &lt;= &amp;EndDate)}
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	Visits.Outlet,
	Visits.Date,
	Visits.SKU,
	Visits.Answer,
	Visits.Brand,
	Visits.SKUGroup,
	Visits.Class,
	Visits.Type,
	Visits.Distributor,
	OutletParameters.Value,
	Visits.SR
FROM
	Visits AS Visits
		LEFT JOIN Catalog.Outlet.Parameters AS OutletParameters
		ON Visits.Outlet = OutletParameters.Ref
{WHERE
	(OutletParameters.Parameter = &amp;Parameter)}</query>
