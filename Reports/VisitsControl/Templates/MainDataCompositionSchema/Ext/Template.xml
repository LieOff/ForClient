		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	BEGINOFPERIOD(VisitPlanOutlets.Date, DAY) AS Date,
	VisitPlanOutlets.Ref.SR AS SR,
	COUNT(DISTINCT VisitPlanOutlets.LineNumber) AS VisitPlansQty,
	VisitPlanOutlets.Outlet,
	VisitPlanOutlets.Ref
INTO VisitPlans
FROM
	Document.VisitPlan.Outlets AS VisitPlanOutlets
{WHERE
	(VisitPlanOutlets.Date &gt;= &amp;BeginDate),
	(VisitPlanOutlets.Date &lt;= &amp;EndDate)}

GROUP BY
	BEGINOFPERIOD(VisitPlanOutlets.Date, DAY),
	VisitPlanOutlets.Ref.SR,
	VisitPlanOutlets.Outlet,
	VisitPlanOutlets.Ref
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	BEGINOFPERIOD(Visit.Date, DAY) AS Date,
	Visit.Plan,
	Visit.SR,
	SUM(CASE
			WHEN Visit.Plan = VALUE(Document.VisitPlan.EmptyRef)
				THEN 0
			ELSE 1
		END) AS PlannedVisitsQty,
	SUM(CASE
			WHEN NOT Visit.Plan = VALUE(Document.VisitPlan.EmptyRef)
				THEN 0
			ELSE 1
		END) AS UnplannedVisitsQty,
	Visit.Outlet,
	Visit.Ref
INTO Visits
FROM
	Document.Visit AS Visit
{WHERE
	(Visit.Date &gt;= &amp;BeginDate),
	(Visit.Date &lt;= &amp;EndDate)}

GROUP BY
	Visit.SR,
	BEGINOFPERIOD(Visit.Date, DAY),
	Visit.Plan,
	Visit.Outlet,
	Visit.Ref
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	ISNULL(VisitPlans.Date, Visits.Date) AS Date,
	ISNULL(VisitPlans.SR, Visits.SR) AS SR,
	ISNULL(VisitPlans.VisitPlansQty, 0) AS VisitPlansQty,
	CASE
		WHEN VisitPlans.Ref IS NULL 
				AND NOT Visits.Ref IS NULL 
			THEN 0
		ELSE ISNULL(Visits.PlannedVisitsQty, 0)
	END AS PlannedVisitsQty,
	CASE
		WHEN VisitPlans.Ref IS NULL 
				AND NOT Visits.Ref IS NULL 
			THEN 1
		ELSE ISNULL(Visits.UnplannedVisitsQty, 0)
	END AS UnplannedVisitsQty,
	ISNULL(VisitPlans.Outlet, Visits.Outlet) AS Outlet,
	Visits.Ref AS Visit,
	VisitPlans.Date AS VisitPlanDate,
	CASE
		WHEN VisitPlans.Ref IS NULL 
				AND NOT Visits.Ref IS NULL 
			THEN CASE
					WHEN NestedSelect.PhotosQty &gt; 0
						THEN 4
					ELSE 5
				END
		WHEN NOT VisitPlans.Ref IS NULL 
				AND NOT Visits.Ref IS NULL 
			THEN CASE
					WHEN NestedSelect.PhotosQty &gt; 0
						THEN 2
					ELSE 3
				END
		WHEN NOT VisitPlans.Ref IS NULL 
				AND Visits.Ref IS NULL 
			THEN 1
	END AS VisitType,
	ISNULL(NestedSelect.PhotosQty, 0) AS PhotosQty,
	0 AS Title,
	Visits.Date AS VisitDate,
	VisitPlans.Ref AS VisitPlan
INTO VisitsPlanVisits
FROM
	VisitPlans AS VisitPlans
		FULL JOIN Visits AS Visits
			LEFT JOIN (SELECT
				VisitQuestions.Ref AS Ref,
				COUNT(DISTINCT ISNULL(VisitQuestions.Question.AnswerType, 0)) AS PhotosQty
			FROM
				Document.Visit.Questions AS VisitQuestions
			WHERE
				VisitQuestions.Question.AnswerType = VALUE(Enum.DataType.Snapshot)
			{WHERE
				(VisitQuestions.Ref.Date &gt;= &amp;BeginDate),
				(VisitQuestions.Ref.Date &lt;= &amp;EndDate)}
			
			GROUP BY
				VisitQuestions.Ref) AS NestedSelect
			ON Visits.Ref = NestedSelect.Ref
		ON VisitPlans.Ref = Visits.Plan
			AND VisitPlans.Date = Visits.Date
			AND VisitPlans.SR = Visits.SR
			AND VisitPlans.Outlet = Visits.Outlet

GROUP BY
	Visits.Ref,
	VisitPlans.Date,
	Visits.Date,
	VisitPlans.Ref,
	ISNULL(NestedSelect.PhotosQty, 0),
	ISNULL(VisitPlans.SR, Visits.SR),
	ISNULL(VisitPlans.VisitPlansQty, 0),
	CASE
		WHEN VisitPlans.Ref IS NULL 
				AND NOT Visits.Ref IS NULL 
			THEN 0
		ELSE ISNULL(Visits.PlannedVisitsQty, 0)
	END,
	CASE
		WHEN VisitPlans.Ref IS NULL 
				AND NOT Visits.Ref IS NULL 
			THEN 1
		ELSE ISNULL(Visits.UnplannedVisitsQty, 0)
	END,
	ISNULL(VisitPlans.Outlet, Visits.Outlet),
	CASE
		WHEN VisitPlans.Ref IS NULL 
				AND NOT Visits.Ref IS NULL 
			THEN CASE
					WHEN NestedSelect.PhotosQty &gt; 0
						THEN 4
					ELSE 5
				END
		WHEN NOT VisitPlans.Ref IS NULL 
				AND NOT Visits.Ref IS NULL 
			THEN CASE
					WHEN NestedSelect.PhotosQty &gt; 0
						THEN 2
					ELSE 3
				END
		WHEN NOT VisitPlans.Ref IS NULL 
				AND Visits.Ref IS NULL 
			THEN 1
	END
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	VisitsPlanVisits.Date,
	VisitsPlanVisits.SR,
	VisitsPlanVisits.VisitPlansQty,
	VisitsPlanVisits.PlannedVisitsQty,
	VisitsPlanVisits.UnplannedVisitsQty,
	VisitsPlanVisits.Outlet,
	VisitsPlanVisits.Visit,
	VisitsPlanVisits.VisitPlanDate,
	VisitsPlanVisits.VisitType,
	VisitsPlanVisits.PhotosQty,
	VisitsPlanVisits.Title,
	VisitsPlanVisits.VisitDate,
	VisitsPlanVisits.VisitPlan,
	VisitsPlanVisits.Outlet.Distributor,
	VisitsPlanVisits.Outlet.Class,
	VisitsPlanVisits.Outlet.Type
FROM
	VisitsPlanVisits AS VisitsPlanVisits</query>
