	<calculatedField>
		<dataPath>CalcVisitType</dataPath>
		<expression>Case
	When Level() = 0
		Then Format(IsNull(Sum(EvalExpressionWithGroupArray("Sum(VisitPlansQty)", "Date, SR")), 0), "NZ=0") + " / " + Format(IsNull(Sum(EvalExpressionWithGroupArray("Sum(PlannedVisitsQty)", "Date, SR")), 0), "NZ=0") + " / " + Format(IsNull(Sum(EvalExpressionWithGroupArray("Sum(UnplannedVisitsQty)", "Date, SR")), 0), "NZ=0")
	Else Case
			When BeginOfPeriod(Date, "Day") = BeginOfPeriod(VisitDate, "Day") Or BeginOfPeriod(Date, "Day") = BeginOfPeriod(VisitPlanDate, "Day")
				Then VisitType
		End
End</expression>
		<title xsi:type="v8:LocalStringType">
