		<query>SELECT ALLOWED
	TerritoryOutlets.Ref.Owner AS DS2Region,
	TerritoryOutlets.Outlet AS DS2Outlet,
	TerritoryOutlets.Ref AS DS2Territory,
	RegionManagers.Manager AS DS2Manager
