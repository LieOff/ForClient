		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED DISTINCT
	QuestionsInQuestionnairesSliceLast.Questionnaire AS Questionnaire,
	QuestionsInQuestionnairesSliceLast.Questionnaire.BeginDate,
	QuestionsInQuestionnairesSliceLast.Questionnaire.EndDate,
	QuestionsInQuestionnairesSliceLast.ChildQuestion,
	SKUsInQuestionnairesSliceLast.SKU
INTO ChildQuestions
FROM
	InformationRegister.QuestionsInQuestionnaires.SliceLast(, ) AS QuestionsInQuestionnairesSliceLast
		INNER JOIN InformationRegister.SKUsInQuestionnaires.SliceLast(, ) AS SKUsInQuestionnairesSliceLast
		ON QuestionsInQuestionnairesSliceLast.Questionnaire = SKUsInQuestionnairesSliceLast.Questionnaire
WHERE
	QuestionsInQuestionnairesSliceLast.ParentQuestion.Assignment = VALUE(Enum.SKUQuestions.Promo)
	AND NOT QuestionsInQuestionnairesSliceLast.Status = VALUE(Enum.ValueTableRowStatuses.Deleted)
	AND NOT SKUsInQuestionnairesSliceLast.Status = VALUE(Enum.ValueTableRowStatuses.Deleted)
{WHERE
	(CASE
			WHEN &amp;EndDate = DATETIME(1, 1, 1)
				THEN TRUE
			ELSE QuestionsInQuestionnairesSliceLast.Questionnaire.BeginDate &lt;= &amp;EndDate
					AND SKUsInQuestionnairesSliceLast.Questionnaire.BeginDate &lt;= &amp;EndDate
		END),
	(CASE
			WHEN &amp;BeginDate = DATETIME(1, 1, 1)
				THEN TRUE
			ELSE CASE
					WHEN QuestionsInQuestionnairesSliceLast.Questionnaire.EndDate = DATETIME(1, 1, 1)
						THEN TRUE
					ELSE QuestionsInQuestionnairesSliceLast.Questionnaire.EndDate &gt;= &amp;BeginDate
							AND SKUsInQuestionnairesSliceLast.Questionnaire.EndDate &gt;= &amp;BeginDate
				END
		END)}
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED DISTINCT
	QuestioningResults.Questionnaire AS Questionnaire,
	QuestioningResults.Visit AS Visit,
	QuestioningResults.Outlet,
	ChildQuestions.ChildQuestion,
	ChildQuestions.SKU
INTO QuestionsVisits
FROM
	ChildQuestions AS ChildQuestions
		INNER JOIN InformationRegister.QuestioningResults AS QuestioningResults
		ON ChildQuestions.Questionnaire = QuestioningResults.Questionnaire
			AND ChildQuestions.QuestionnaireBeginDate &lt;= QuestioningResults.Date
			AND (CASE
				WHEN ChildQuestions.QuestionnaireEndDate = DATETIME(1, 1, 1)
					THEN TRUE
				ELSE ChildQuestions.QuestionnaireEndDate &gt;= QuestioningResults.Date
			END)
{WHERE
	(QuestioningResults.Date &lt;= &amp;EndDate),
	(QuestioningResults.Date &gt;= &amp;BeginDate)}
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED DISTINCT
	AnswersSliceLast.Period AS Date,
	AnswersSliceLast.Question AS ChildQuestion,
	AnswersSliceLast.Outlet,
	AnswersSliceLast.SKU,
	QuestioningResults.Questionnaire,
	AnswersSliceLast.Visit AS Visit,
	AnswersSliceLast.Answer,
	QuestionsVisits.Visit.SR AS SR
INTO Answers
FROM
	InformationRegister.Answers.SliceLast(, ) AS AnswersSliceLast
		INNER JOIN QuestionsVisits AS QuestionsVisits
		ON AnswersSliceLast.Visit = QuestionsVisits.Visit
			AND AnswersSliceLast.Outlet = QuestionsVisits.Outlet
			AND AnswersSliceLast.SKU = QuestionsVisits.SKU
			AND AnswersSliceLast.Question = QuestionsVisits.ChildQuestion
		INNER JOIN InformationRegister.QuestioningResults AS QuestioningResults
		ON AnswersSliceLast.Visit = QuestioningResults.Visit
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	BEGINOFPERIOD(Answers.Date, DAY) AS Date,
	Answers.ChildQuestion,
	MAX(Answers.Outlet) AS Outlet,
	Answers.SKU,
	Answers.Questionnaire,
	Answers.Visit,
	Answers.Answer,
	Answers.SR,
	OutletParameters.Value,
	Answers.Outlet.Distributor,
	Answers.Outlet.Class,
	Answers.Outlet.Type,
	Answers.SKU.Brand AS Brand,
	Answers.SKU.Owner AS SKUGroup
FROM
	Answers AS Answers
		INNER JOIN Catalog.Outlet.Parameters AS OutletParameters
		ON Answers.Outlet = OutletParameters.Ref
WHERE
	OutletParameters.Parameter = &amp;Parameter

GROUP BY
	Answers.SR,
	Answers.ChildQuestion,
	Answers.SKU,
	Answers.Questionnaire,
	Answers.Visit,
	Answers.Answer,
	OutletParameters.Value,
	Answers.Outlet.Distributor,
	Answers.Outlet.Class,
	Answers.Outlet.Type,
	BEGINOFPERIOD(Answers.Date, DAY),
	Answers.SKU.Brand,
	Answers.SKU.Owner</query>
