		<dataSource>DataSource1</dataSource>
		<query>SELECT
	MAX(AnswersSliceLast.Period) AS Period,
	AnswersSliceLast.SKU,
	AnswersSliceLast.Outlet
INTO MaxPeriodSKUs
FROM
	InformationRegister.Answers.SliceLast AS AnswersSliceLast
WHERE
	AnswersSliceLast.QuestionAssignment = VALUE(Enum.SKUQuestions.Facing)

GROUP BY
	AnswersSliceLast.SKU,
	AnswersSliceLast.Outlet
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	Answers.Outlet AS Outlet,
	Answers.SKU AS SKU,
	AVG(CAST(Answers.Answer AS NUMBER)) AS AvgStock
INTO Average
FROM
	InformationRegister.Answers AS Answers
WHERE
	Answers.QuestionAssignment = VALUE(Enum.SKUQuestions.Facing)
{WHERE
	(Answers.Period &lt;= &amp;Period) AS Field1,
	(Answers.Period &gt;= DATEADD(&amp;Period, MONTH, -2)) AS Field3}

GROUP BY
	Answers.Outlet,
	Answers.SKU

INDEX BY
	Outlet,
	SKU
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	AnswersSliceLast.Outlet AS Outlet,
	AnswersSliceLast.SKU AS SKU,
	CAST(AnswersSliceLast.Answer AS NUMBER) AS Stock,
	AnswersSliceLast.Visit.SR AS VisitSR
INTO AllBrands
FROM
	InformationRegister.Answers.SliceLast({(&amp;Period)}, QuestionAssignment = VALUE(Enum.SKUQuestions.Facing)) AS AnswersSliceLast
		INNER JOIN MaxPeriodSKUs AS MaxPeriodSKUs
		ON AnswersSliceLast.SKU = MaxPeriodSKUs.SKU
			AND AnswersSliceLast.Outlet = MaxPeriodSKUs.Outlet
			AND AnswersSliceLast.Period = MaxPeriodSKUs.Period

INDEX BY
	Outlet,
	SKU,
	VisitSR
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	AnswersSliceLast.Outlet AS Outlet,
	AnswersSliceLast.SKU AS SKU,
	CAST(AnswersSliceLast.Answer AS NUMBER) AS CurrentBrandStock,
	AnswersSliceLast.SKU.Brand AS SKUBrand
INTO CurrentBrand
FROM
	InformationRegister.Answers.SliceLast({(&amp;Period)}, QuestionAssignment = VALUE(Enum.SKUQuestions.Facing)) AS AnswersSliceLast
		INNER JOIN MaxPeriodSKUs AS MaxPeriodSKUs
		ON AnswersSliceLast.Outlet = MaxPeriodSKUs.Outlet
			AND AnswersSliceLast.SKU = MaxPeriodSKUs.SKU
			AND AnswersSliceLast.Period = MaxPeriodSKUs.Period

INDEX BY
	Outlet,
	SKU,
	SKUBrand
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	AnswersSliceLast.Outlet AS Outlet,
	AnswersSliceLast.SKU AS SKU,
	SUM(CAST(AnswersSliceLast.Answer AS NUMBER)) AS TrueCurrBrandStock
INTO TrueCurrBrand
FROM
	InformationRegister.Answers.SliceLast({(&amp;Period)}, QuestionAssignment = VALUE(Enum.SKUQuestions.Facing)) AS AnswersSliceLast
		INNER JOIN MaxPeriodSKUs AS MaxPeriodSKUs
		ON AnswersSliceLast.SKU = MaxPeriodSKUs.SKU
			AND AnswersSliceLast.Outlet = MaxPeriodSKUs.Outlet
			AND AnswersSliceLast.Period = MaxPeriodSKUs.Period
WHERE
	AnswersSliceLast.SKU.Brand = &amp;Brand

GROUP BY
	AnswersSliceLast.Outlet,
	AnswersSliceLast.SKU

INDEX BY
	Outlet,
	SKU
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	AllBrands.Outlet,
	SUM(AllBrands.Stock) AS SKUGroupStock,
	AllBrands.SKU.Owner AS SKUGroup
INTO SKUGroups
FROM
	AllBrands AS AllBrands

GROUP BY
	AllBrands.SKU.Owner,
	AllBrands.Outlet
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	NestedSelect.Region,
	NestedSelect.Territory,
	AllBrands.Outlet AS Outlet,
	AllBrands.SKU.Brand,
	AllBrands.SKU AS SKU,
	AllBrands.Stock,
	CurrentBrand.CurrentBrandStock,
	0 AS Percent,
	Average.AvgStock,
	OutletParameters.Value,
	AllBrands.VisitSR AS SR,
	ISNULL(TrueCurrBrand.TrueCurrBrandStock, 0) AS TrueCurrBrandStock,
	SKUGroups.SKUGroup,
	SKUGroups.SKUGroupStock
FROM
	AllBrands AS AllBrands
		LEFT JOIN Average AS Average
		ON AllBrands.Outlet = Average.Outlet
			AND AllBrands.SKU = Average.SKU
		LEFT JOIN (SELECT
			TerritoryOutlets.Ref.Owner AS Region,
			TerritoryOutlets.Ref AS Territory,
			TerritoryOutlets.Outlet AS Outlet
		FROM
			Catalog.Territory.Outlets AS TerritoryOutlets) AS NestedSelect
		ON AllBrands.Outlet = NestedSelect.Outlet
		LEFT JOIN Catalog.Outlet.Parameters AS OutletParameters
		ON AllBrands.Outlet = OutletParameters.Ref
		LEFT JOIN CurrentBrand AS CurrentBrand
		ON AllBrands.Outlet = CurrentBrand.Outlet
			AND AllBrands.SKU = CurrentBrand.SKU
			AND AllBrands.SKU.Brand = CurrentBrand.SKUBrand
		LEFT JOIN TrueCurrBrand AS TrueCurrBrand
		ON AllBrands.Outlet = TrueCurrBrand.Outlet
			AND AllBrands.SKU = TrueCurrBrand.SKU
		LEFT JOIN SKUGroups AS SKUGroups
		ON AllBrands.Outlet = SKUGroups.Outlet
			AND AllBrands.SKU.Owner = SKUGroups.SKUGroup
WHERE
	OutletParameters.Parameter = &amp;Parameter</query>
